{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "ai4-dashboard": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/ai4-dashboard",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": ["zone.js"],
                        "tsConfig": "tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": ["src/assets"],
                        "stylePreprocessorOptions": {
                            "includePaths": ["src/theme/exports"]
                        },
                        "scripts": []
                    },
                    "configurations": {
                        "production-ai4eosc": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "2mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ],
                            "outputHashing": "all",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.production.ts"
                                }
                            ],
                            "scripts": [
                                "node_modules/cookieconsent/build/cookieconsent.min.js"
                            ],
                            "styles": [
                                "src/theme/ai4eosc/variables.scss",
                                "src/styles.scss",
                                "node_modules/ol/ol.css",
                                "node_modules/cookieconsent/build/cookieconsent.min.css"
                            ],
                            "assets": [
                                {
                                    "glob": "**/*",
                                    "input": "src/assets/images/ai4eosc",
                                    "output": "/assets/images/"
                                },
                                {
                                    "glob": "config.json",
                                    "input": "src/assets/config/ai4eosc/",
                                    "output": "/assets/"
                                },
                                {
                                    "glob": "**/*",
                                    "input": "src/assets/",
                                    "output": "/assets/"
                                }
                            ]
                        },
                        "production-imagine": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "2mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ],
                            "outputHashing": "all",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.production.ts"
                                }
                            ],
                            "scripts": [
                                "node_modules/cookieconsent/build/cookieconsent.min.js"
                            ],
                            "styles": [
                                "src/theme/imagine/variables.scss",
                                "src/styles.scss",
                                "node_modules/ol/ol.css",
                                "node_modules/cookieconsent/build/cookieconsent.min.css"
                            ],
                            "assets": [
                                {
                                    "glob": "**/*",
                                    "input": "src/assets/images/imagine",
                                    "output": "/assets/images/"
                                },
                                {
                                    "glob": "config.json",
                                    "input": "src/assets/config/imagine/",
                                    "output": "/assets/"
                                },
                                {
                                    "glob": "**/*",
                                    "input": "src/assets/",
                                    "output": "/assets/"
                                }
                            ]
                        },
                        "development-ai4eosc": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true,
                            "outputHashing": "all",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.development.ts"
                                }
                            ],
                            "scripts": [
                                "node_modules/cookieconsent/build/cookieconsent.min.js"
                            ],
                            "styles": [
                                "src/theme/ai4eosc/variables.scss",
                                "src/styles.scss",
                                "node_modules/ol/ol.css",
                                "node_modules/cookieconsent/build/cookieconsent.min.css"
                            ],
                            "assets": [
                                {
                                    "glob": "**/*",
                                    "input": "src/assets/images/ai4eosc",
                                    "output": "/assets/images/"
                                },
                                {
                                    "glob": "config.json",
                                    "input": "src/assets/config/ai4eosc/",
                                    "output": "/assets/"
                                },
                                {
                                    "glob": "**/*",
                                    "input": "src/assets/",
                                    "output": "/assets/"
                                }
                            ]
                        },
                        "development-imagine": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true,
                            "outputHashing": "all",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.development.ts"
                                }
                            ],
                            "scripts": [
                                "node_modules/cookieconsent/build/cookieconsent.min.js"
                            ],
                            "styles": [
                                "src/theme/imagine/variables.scss",
                                "src/styles.scss",
                                "node_modules/ol/ol.css",
                                "node_modules/cookieconsent/build/cookieconsent.min.css"
                            ],
                            "assets": [
                                {
                                    "glob": "**/*",
                                    "input": "src/assets/images/imagine",
                                    "output": "/assets/images/"
                                },
                                {
                                    "glob": "config.json",
                                    "input": "src/assets/config/imagine/",
                                    "output": "/assets/"
                                },
                                {
                                    "glob": "**/*",
                                    "input": "src/assets/",
                                    "output": "/assets/"
                                }
                            ]
                        },
                        "local-ai4eosc": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.local.ts"
                                }
                            ],
                            "scripts": [
                                "node_modules/cookieconsent/build/cookieconsent.min.js"
                            ],
                            "styles": [
                                "src/theme/ai4eosc/variables.scss",
                                "src/styles.scss",
                                "node_modules/ol/ol.css",
                                "node_modules/cookieconsent/build/cookieconsent.min.css"
                            ],
                            "assets": [
                                {
                                    "glob": "**/*",
                                    "input": "src/assets/images/ai4eosc",
                                    "output": "/assets/images/"
                                },
                                {
                                    "glob": "config.json",
                                    "input": "src/assets/config/ai4eosc/",
                                    "output": "/assets/"
                                },
                                {
                                    "glob": "**/*",
                                    "input": "src/assets/",
                                    "output": "/assets/"
                                }
                            ]
                        },
                        "local-imagine": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.local.ts"
                                }
                            ],
                            "scripts": [
                                "node_modules/cookieconsent/build/cookieconsent.min.js"
                            ],
                            "styles": [
                                "src/theme/imagine/variables.scss",
                                "src/styles.scss",
                                "node_modules/ol/ol.css",
                                "node_modules/cookieconsent/build/cookieconsent.min.css"
                            ],
                            "assets": [
                                {
                                    "glob": "**/*",
                                    "input": "src/assets/images/imagine",
                                    "output": "/assets/images/"
                                },
                                {
                                    "glob": "config.json",
                                    "input": "src/assets/config/imagine/",
                                    "output": "/assets/"
                                },
                                {
                                    "glob": "**/*",
                                    "input": "src/assets/",
                                    "output": "/assets/"
                                }
                            ]
                        },
                        "tutorials-ai4eosc": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "2mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ],
                            "outputHashing": "all",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.production.ts"
                                }
                            ],
                            "scripts": [
                                "node_modules/cookieconsent/build/cookieconsent.min.js"
                            ],
                            "styles": [
                                "src/theme/ai4eosc/variables.scss",
                                "src/styles.scss",
                                "node_modules/ol/ol.css",
                                "node_modules/cookieconsent/build/cookieconsent.min.css"
                            ],
                            "assets": [
                                {
                                    "glob": "**/*",
                                    "input": "src/assets/images/ai4eosc",
                                    "output": "/assets/images/"
                                },
                                {
                                    "glob": "config.json",
                                    "input": "src/assets/config/tutorials/",
                                    "output": "/assets/"
                                },
                                {
                                    "glob": "**/*",
                                    "input": "src/assets/",
                                    "output": "/assets/"
                                }
                            ]
                        },
                        "ai4life-ai4eosc": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "2mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ],
                            "outputHashing": "all",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.production.ts"
                                }
                            ],
                            "scripts": [
                                "node_modules/cookieconsent/build/cookieconsent.min.js"
                            ],
                            "styles": [
                                "src/theme/ai4life/variables.scss",
                                "src/styles.scss",
                                "node_modules/ol/ol.css",
                                "node_modules/cookieconsent/build/cookieconsent.min.css"
                            ],
                            "assets": [
                                {
                                    "glob": "**/*",
                                    "input": "src/assets/images/ai4life",
                                    "output": "/assets/images/"
                                },
                                {
                                    "glob": "config.json",
                                    "input": "src/assets/config/ai4life/",
                                    "output": "/assets/"
                                },
                                {
                                    "glob": "**/*",
                                    "input": "src/assets/",
                                    "output": "/assets/"
                                }
                            ]
                        }
                    },
                    "defaultConfiguration": "production-ai4eosc"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "local-ai4eosc": {
                            "browserTarget": "ai4-dashboard:build:local-ai4eosc"
                        },
                        "production-ai4eosc": {
                            "browserTarget": "ai4-dashboard:build:production-ai4eosc"
                        },
                        "development-ai4eosc": {
                            "browserTarget": "ai4-dashboard:build:development-ai4eosc"
                        },
                        "local-imagine": {
                            "browserTarget": "ai4-dashboard:build:local-imagine"
                        },
                        "production-imagine": {
                            "browserTarget": "ai4-dashboard:build:production-imagine"
                        },
                        "development-imagine": {
                            "browserTarget": "ai4-dashboard:build:development-imagine"
                        },
                        "tutorials-ai4eosc": {
                            "browserTarget": "ai4-dashboard:build:tutorials-ai4eosc"
                        },
                        "ai4life-ai4eosc": {
                            "browserTarget": "ai4-dashboard:build:ai4life-ai4eosc"
                        }
                    },
                    "defaultConfiguration": "local-ai4eosc"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "ai4-dashboard:build"
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
                    }
                }
            }
        }
    },
    "cli": {
        "schematicCollections": ["@angular-eslint/schematics"],
        "analytics": false
    }
}
